<!-------------PREVIEW  AND  SUBMIT SECTION  ------------->
<div class="preview-submit-wrapper">
    <div class="preview-submit-comntainer">
        <form action="" method="POST" class="form-preview-submit" id="form-preview-submit">
            <h3> Preview </h3>
            <div class="preview-close-btn"><div class="i"></div></div>
            <div class="preview-content-wrapper">
                <div class="preview-content-container">
                    <!------------- EJS CODES SECTION  -------------------->
                    <% if (databasePreview !== undefined || databasePreview !== null) { %>
                        <% if (databasePreview == "404") { %>
                            <div class="preview-empty-content">
                                <div class="empty-preview"> Data is Empty </div>
                            </div>
                        <% } else { %>
                            <div class="show-preview-wrap">

                                
                                <div class="preview-headers">
                                    <div>
                                        <div class="preview-collapse"></div>
                                        <h5> Company Name </h5>
                                        <div class="i file-icon"><div></div></div>
                                    </div>
                                    <div class="sub-headers">
                                        <ul>
                                            <li> Total : <span> 60 </span></li>
                                            <li> Pending : <span> 40 </span></li>
                                            <li> Remainig : <span> 20 </span></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="show-preview-content">
                                    <div class="show-content show-main-content" id="show-main-content">
                                        <div class="show-content-headers">
                                            <div class="preview-collapse" data-arrow="main"></div>
                                            <h5> Main </h5>
                                        </div>
                                        <div class="show-data-content">
                                            <div class="show-data-image"><div class="i"><img src="" alt=""></div></div>
                                            <ul class="show-data show-data-main" data-content="main">
                                                <li> Key :  <span> value </span></li>
                                                <li> Key :  <span> value </span></li>
                                                <li> Key :  <span> value </span></li>
                                                <li> Key :  <span> value </span></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        <% } %>
                    <% } %>
                    <!---------------------------------------------------->
                </div>
            </div>


            <button class="preview-submit-btn"> Submit To Database </button>
        </form>
    </div>
</div>
