    <script src="./js/sw-register.js"></script>
    -->
    <script src="./js/landing-page.js"></script>
    <script src="./js/serverMessage/server-checkout.js"></script>
    <!--<script src="./js/plugins/firebase-app-init.js" type="module" async></script>-->
    <script src="./js/customer_details.js" type="module"></script>
